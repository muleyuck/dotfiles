{
  "Print to console": {
    "prefix": "clg",
    "body": [
      "console.log($1)"
    ]
  },
  "Print to console with divide": {
    "prefix": "clgd",
    "body": [
      "console.log(\"====================\")",
      "console.log(\"$1\", $2)",
      "console.log(\"====================\")"
    ]
  },
  "try-catch-finally": {
    "prefix": "tcf",
    "body": [
      "try {",
      "  $1",
      "} catch (err) {",
      "  console.log(err)",
      "} finally {",
      "  ",
      "}"
    ]
  },
  "promise and callbacks": {
    "prefix": "ptcf",
    "body": [
      "${1:promise}.then((resp) => {",
      "  ",
      "}).catch((err) => {",
      "  console.log(err)",
      "}).finally(() => {",
      "  ",
      "})"
    ]
  },
  "axios request": {
    "prefix": "axs",
    "body": [
      "axios.${1|get,post,put|}(`$2`, {})"
    ]
  },
  "axios request await": {
    "prefix": "axsa",
    "body": [
      "await axios.${1|get,post,put|}(`$2`, {})"
    ]
  },
  "sleep": {
    "prefix": "sleep",
    "body": [
      "const sleep = (waitTime: number) => {",
      "  return new Promise((resolve) => setTimeout(resolve, waitTime))",
      "}",
      "$1"
    ]
  },
  "array map =>": {
    "prefix": "mp",
